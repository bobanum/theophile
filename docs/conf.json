{
    "plugins": [],
    "recurseDepth": 10,
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "destination": "./docs/api",
        "recurse": true,
        "verbose": true
    },
    "source": {
        "off-include": ["."],
        "exclude": ["docs", "example"],
        "off-includePattern": ".+\\.js(doc|x)?$",
        "off-excludePattern": "(^|\\/|\\\\)_"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}

